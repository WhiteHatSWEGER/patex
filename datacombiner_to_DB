# Insert sensor data into the local LibreOffice Base database
def insert_sensor_data(sensor_data):
    conn = get_db_connection()
    for measurement in sensor_data:
        conn.execute('INSERT INTO sensor_data (measurement, timestamp, value) VALUES (?, ?, ?)',
                     (measurement, datetime.now(), sensor_data[measurement]))
    conn.commit()
    conn.close()

# Combine sensor data into a single dictionary
sensor_data = {
    "MQ135": get_mq135_data(),
    "MQ2": get_mq2_data(),
    "MQ3": get_mq3_data(),
    "MQ4": get_mq4_data(),
    "MQ5": get_mq5_data(),
    "MQ6": get_mq6_data(),
    "MQ7": get_mq7_data(),
    "MQ8": get_mq8_data(),
    "MQ9": get_mq9_data()
}

# Send sensor data to the local LibreOffice Base database
insert_sensor_data(sensor_data)
